RewriteRule ^(.*)$ $1.html
RewriteCond %{HTTP_HOST} ^bemfasilkomunsika\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.bemfasilkomunsika\.com$
RewriteRule ^/?$ "https\:\/\/bem\.cs\.unsika\.ac\.id\/" [R=301,L]
RewriteCond %{HTTPS} off
